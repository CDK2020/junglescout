<script type="text/javascript">
    P.when('A', 'smart-shelf-desktop', 'action-panel-secondary-panel', 'twister-aui-page-state').execute(function (A, smartShelfDesktop, secondaryPanel, twisterPageState) {
        twisterPageState.updatePageState();
        var secondaryPanelSettings = A.state("ccxsmartShelfSecondaryPanelSettings");
        secondaryPanel.init(secondaryPanelSettings);
        if (typeof smartShelfDesktop.initV2 !== "undefined" && typeof smartShelfDesktop.initV2 === "function") {
            smartShelfDesktop.initV2(secondaryPanel, secondaryPanelSettings.panelId, "#add-to-cart-button", "offerListingID");
        } else {
            smartShelfDesktop.init(secondaryPanel, secondaryPanelSettings.panelId, "#add-to-cart-button");
        }
    });
</script>

