            <div id="bRecDataDiv">
                <script type="a-state" data-a-state="{&quot;key&quot;:&quot;beautyRecommendations&quot;}">{"numRecommendations":2,"possibleSums":["$7.61","$13.60","$15.56","$21.55"]}</script>
            </div>















            <script type="a-state" data-a-state="{&quot;key&quot;:&quot;bRecDynamicStrings&quot;}">{"bRecCart3":"Add all three to Cart","bRecCart2":"Add both to Cart","bRecCart4":"Add all four to Cart","bRecPrice2":"Price for both:","bRecPrice1":"Price for single item:","bRecCart1":"Add to Cart","bRecPrice4":"Price for all four:","bRecPrice3":"Price for all three:","bRecWish4":"Add all four to Wish List","bRecWish3":"Add all three to Wish List","bRecWish2":"Add both to Wish List","bRecWish1":"Add to Wish List"}</script>

        </div>
    </div>
    <script type="text/javascript">

    // This block will fire quickly (on critical feature) to do the necessary
    // DOM transformation right away.
    P.when('jQuery', 'cf').execute(function($) {

        "use strict";

        // A.state is not available yet, so just count dom elements.
        var numRecs = $(".bRecItemImage").length;

        var animationParentEl = $("#beautyRecommendations");

        function switchImage(image, switchToSmall) {
            if(switchToSmall){
                var smallImageUrl=image.data("itemSmallImage");
                image.css("background",smallImageUrl);
            }else{
                var imageUrl=image.data("itemImage");
                image.css("background",imageUrl);
            }
        }

        function updateImage() {
            var window=$(this);
            var switchToSmall=false;
            //responsiveHelper is visible means now it's small view port
            if($("#responsiveHelper").is(':visible')){
                switchToSmall=true;
            }

            switchImage($("#rootItemImage"),switchToSmall);
            $(".bRecImage").each(function(){
                switchImage($(this),switchToSmall);
            });
        }

        function init() {

            for (var i = 0; i < numRecs; i++) {
                // Force it to be checked (e.g. after page refresh)
                $("#bRecCheckbox" + i).prop('checked', true);
            }
            updateImage();
            $(window).resize(updateImage);
        }

        init();

    });


    // Need to wait for 'ready' to ensure A.state will have the information ready.
    P.when('A', 'jQuery', 'twister-aui-page-state', 'ready').execute(function(A, $, twisterPageState){

        "use strict";

        twisterPageState.updatePageState();
        var jsonData = A.state("beautyRecommendations");
        var dynamicStrings = A.state('bRecDynamicStrings');

        if (!jsonData) {
            return;
        }

        var numRecs = jsonData.numRecommendations;
        var sumPermutations = jsonData.possibleSums;
        var formAction=$("#bRecForm").attr('action');

        function getCartString(num) {
            switch (num) {
                case 0: return dynamicStrings.bRecCart1;
                case 1: return dynamicStrings.bRecCart2;
                case 2: return dynamicStrings.bRecCart3;
                case 3: return dynamicStrings.bRecCart4;
                default: return dynamicStrings.bRecCart1;
            }
        }

        function getListString(num) {
            switch (num) {
                case 0: return dynamicStrings.bRecWish1;
                case 1: return dynamicStrings.bRecWish2;
                case 2: return dynamicStrings.bRecWish3;
                case 3: return dynamicStrings.bRecWish4;
                default: return dynamicStrings.bRecWish1;
            }
        }

        function getPriceString(num) {
            switch (num) {
                case 0: return dynamicStrings.bRecPrice1;
                case 1: return dynamicStrings.bRecPrice2;
                case 2: return dynamicStrings.bRecPrice3;
                case 3: return dynamicStrings.bRecPrice4;
                default: return dynamicStrings.bRecPrice1;
            }
        }

        function getImage(index) {
            return $("#bRecImg" + index);
        }

        function getCheckbox(index) {
            return $("#bRecCheckbox" + index);
        }

        function isChecked(i) {
            return getCheckbox(i).prop("checked");
        }

        // update price display for current widget state
        function update_price() {

            var permutationNum = ""; // watch this!
            for ( var i = 0; i < numRecs; i++ ) {
                permutationNum += isChecked(i) ? "1" : "0";
            }

            permutationNum = parseInt(permutationNum, 2); // convert binary string to integer

            var formattedPrice = sumPermutations[permutationNum];
            $("#bRecPrice").html(formattedPrice);

        }

        function update() {
            var numChecked = 0,
                    checked,
                    image;

            for (var i = 0; i < numRecs; i++) {

                image = getImage(i);
                checked = isChecked(i);

                if (checked) {
                    numChecked++;
                    image.removeClass("bRecDisabled");
                } else {
                    image.addClass("bRecDisabled");
                }
            }

            $("#bRecAddToCart").find("span.a-button-text").text(getCartString(numChecked));
            $("#bRecAddToList").find("span.a-button-text").text(getListString(numChecked));
            $("#bRecPriceLabel").text(getPriceString(numChecked));

            var numToAddToCart=numChecked+1;
            $("#bRecForm").attr('action', formAction + numToAddToCart);

            update_price();

        }


        function init() {
            for (var i = 0; i < numRecs; i++) {
                // Attach an event listener
                getCheckbox(i).change(update);
            }
            // Undo some provisions for no-script viewing
            $("#bRecPricePara").removeClass("a-hidden");
            update();
        }
        init();
    });

    </script>































    </div>
</div>


